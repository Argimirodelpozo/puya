abstract contract CallCounter
{
  globals {
    ['counter']: algopy.UInt64
  }
  locals {
    ['name']: algopy.Bytes
  }
  
  constructor()
  {
    this.globals['counter']: algopy.UInt64 = 0u
  }
  
  subroutine increment_counter(): None
  {
    this.globals['counter'] += 1u
  }
  
  subroutine set_sender_nickname(nickname: algopy.Bytes): None
  {
    this.locals['name'].account[0u]: algopy.Bytes = nickname
  }
}